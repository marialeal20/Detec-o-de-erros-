import turtle
import math

anne= turtle.Turtle()

anne.color ("black,"pink")
anne.speed("10")

anne.begin_fill()
"""
for i in range(200):
	anne.forward(math.sqrt(i))
	anne.left(i%180)
"""
for i in range(2000):
	anne.forward(10)
	anne.right(math.sin(i/10)*25)
	anne.left(20)

anne.end_fill()



turtle.done()
